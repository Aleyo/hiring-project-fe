{
  "presets": [
    "@babel/react",
    "@babel/typescript",
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "chrome": "58",
          "ie": "11"
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["."],
      "extensions": [".ts", ".tsx"],
    }]
  ]
}
